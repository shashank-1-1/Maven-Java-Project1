pipeline{
    agent any
    
    environment{
        PATH = "/usr/share/maven/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/shashank-1-1/Maven-Java-Project1.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
            }
        
        }
    }
}
